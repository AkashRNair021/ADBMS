DECLARE

N NUMBER := &N;

SUMS NUMBER

:= 0;

REM NUMBER;

TEMP NUMBER := N;

BEGIN

WHILE N0 LOOP

REM : MOD(N, 10);

SUMS: SUMS + REM REM REM;

N: TRUNC(N/10);

END LOOP;

IF SUMS TEMP THEN

DBMS_OUTPUT.PUT_LINE(TEMP || ' IS ARMSTRONG');

ELSE

DBMS_OUTPUT.PUT_LINE(TEMP || ' IS NOT ARMSTRONG');

END IF;

END;
